<body>
    <nav class="navbar sticky navbar-expand-lg navbar-light bg-blue">
        <div class="container-fluid d-flex justify-content-between">
            <a class="navbar-brand logo-bn d-flex" href="/">
                <img src="https://bluenotary.us/assets/img/logo-a4-circle-white.png" alt="Logo BlueNotary"
                    class="d-inline-block">
                <span class="text-white">BlueNotary</span>
            </a>
            <button class="navbar-toggler text-white" type="button" data-bs-toggle="collapse"
                data-bs-target="#navbarNavAltMarkup" aria-controls="navbarNavAltMarkup" aria-expanded="false"
                aria-label="Toggle navigation">
                <span class="btn btn--slim btn-white text-white">Menu</span>
            </button>
            <div class="collapse navbar-collapse float-end" id="navbarNavAltMarkup">
                <div class="navbar-nav">
                    <a class="nav-link btn text-white" aria-current="page" href="how-it-works">How It Works</a>
                    <a class="nav-link btn text-white" href="for-businesses">For Title Agents</a>
                    <a class="nav-link btn text-white" aria-current="page" href="how-it-works">For Individuals</a>
                    <a class="nav-link btn text-white" aria-current="page" href="for-businesses">For Businesses</a>
                    <!-- <a class="nav-link btn text-white" href="for-notaries">For Notaries</a> -->
                    <!-- <a class="nav-link btn btn--slim btn-primary text-white" href="https://app.bluenotary.us/">Login</a> -->
                </div>
            </div>
        </div>
    </nav>

    <section class="container" style="margin-top: 8rem;width:80%">
        <nav style="--bs-breadcrumb-divider: '>';" aria-label="breadcrumb">
            <ol class="breadcrumb">
                <li class="breadcrumb-item"><a href="https://bluenotary.us">Home</a></li>
                <li class="breadcrumb-item active" aria-current="page">All Profile Indexes</li>
            </ol>
        </nav>
        <p><h1 style="font-size: 3rem; margin-bottom: 3rem;">BlueNotary Notaries Index</h1></p>
        <ul class="nav nav-pills">
            <li class="nav-item">
                <span class="nav-link disabled">State list :</span>
            </li>
            <% for (var i=0; i < statesList.length; i++) { %><li class="nav-item">
                <a class="nav-link text-blue" href="<%= statesList[i][1] %>">
                    <%= statesList[i][0] %>
                </a>
            </li><% } %>
        </ul>
        <div id="mainrow" class="flex-row row">
            <% for (var i=0; i < profiles.length; i++) { %>
            <div class="col-sm-4">
                <div class="card carditem">
                    <div style="display: block;">
                        <div class="cardtext">
                            <a href="<%= profiles[i].link %>"><%= profiles[i].name %></a> <br>
                            <% if (profiles[i].cardSubTxt) { %>
                            <span><%- profiles[i].cardSubTxt %></span>
                            <% } %>
                        </div>
                        <div class="cardimage">
                            <% if (profiles[i].avatarUrl) { %>
                                <img src="<%= profiles[i].avatarUrl %>" alt="<%= profiles[i].name %> User image">
                            <% } %>
                        </div>
                    </div>
                </div>
            </div><% } %>
        </div>
    </section>

